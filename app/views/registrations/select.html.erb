<div class="mt-3 sr-content">
  <h1>Please Select Your Registration</h1>
  <%= simple_form_for :registration, url: post_edit_registration_path,
    wrapper: :horizontal_form,
    wrapper_mappings: {
      boolean:       :horizontal_boolean,
      check_boxes:   :horizontal_collection,
      date:          :horizontal_multi_select,
      datetime:      :horizontal_multi_select,
      file:          :horizontal_file,
      radio_buttons: :horizontal_collection,
      range:         :horizontal_range,
      time:          :horizontal_multi_select
    } do |f| %>

  <%= f.input :id, as: :hidden %>
  <%= f.input :value, label: 'Select Registration', input_html: { class: "basicAutoComplete", autocomplete: "off" } %>

  <div class="form-group row mb-0">
    <div class="col-sm-9 offset-sm-3">
      <button class="btn btn-primary">Search</button>
    </div>
  </div>
  <% end %>

</div>
